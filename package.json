{
  "name": "vectoria",
  "version": "0.0.4",
  "type": "module",
  "main": "./dist/vectoria.umd.cjs",
  "module": "./dist/vectoria.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/vectoria.js",
      "require": "./dist/vectoria.umd.cjs",
      "types": "./dist/index.d.ts"
    },
    "./thin": {
      "import": "./dist/vectoria.thin.js",
      "require": "./dist/vectoria.thin.umd.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && vite build --mode thin",
    "build:docs": "tsc && vite build --mode docs && cp -r data docs/",
    "preview": "vite preview",
    "test": "vitest run tests/vectoria.test.ts"
  },
  "dependencies": {
    "@huggingface/transformers": "^3.8.1",
    "idb": "^8.0.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/uuid": "^9.0.8",
    "@types/web": "^0.0.148",
    "fake-indexeddb": "^6.2.5",
    "happy-dom": "^20.0.11",
    "typescript": "^5.4.2",
    "vite": "^5.2.0",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-static-copy": "^3.1.4",
    "vitest": "^4.0.15"
  }
}
